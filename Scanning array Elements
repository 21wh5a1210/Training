#include <iostream>

using namespace std;

int
main ()
{

  int arr[5];

  for (int i = 0; i <= 5; i++)
    {
      cin >> arr[i];

    }
  // for (int i = 0; i <= 5; i++)
  //{
  //cout << arr[i] << endl;
  // }
  int min = 10;
  for (int i = 0; i < 6; i++)
    {
      if (min > arr[i])
	{
	  min = arr[i];

	}
      //cout<<arr[i];
    }
  cout << min;
  return 0;
}
